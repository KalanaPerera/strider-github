{
  "name": "strider-github",
  "version": "0.0.0",
  "description": "A basic git provider for strider",
  "main": "index.js",
  "scripts": {
    "test": "make test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Strider-CD/strider-git-provider.git"
  },
  "keywords": [
    "git",
    "strider"
  ],
  "author": "Jared Forsyth <jared@jaredforsyth.com>",
  "license": "MIT",
  "readmeFilename": "Readme.md",
  "strider": {
    "id": "github",
    "title": "Github",
    "type": "provider",
    "hosted": true,
    "config": true,
    "accountConfig": {
      "setupLink": "/ext/github/oauth"
    },
    "webapp": "lib/webapp.js",
    "worker": "lib/worker.js",
    "inline_icon": "github"
  },
  "devDependencies": {
    "jshint": "~2.1.11",
    "expect.js": "~0.2.0",
    "mocha": "~1.13.0"
  },
  "dependencies": {
    "passport": "~0.1.17",
    "superagent": "~0.15.4",
    "strider-git": "git+https://github.com/Strider-CD/strider-git.git",
    "request": "~2.27.0",
    "lodash": "~2.2.0",
    "passport-github": "~0.1.5",
    "step": "0.0.5"
  }
}
